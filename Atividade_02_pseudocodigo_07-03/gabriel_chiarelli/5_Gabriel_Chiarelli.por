programa {
  funcao inicio() { 
    /* 
Desenvolva um pseudoc´odigo que solicite dois n´umeros ao usu´ario, calcule a soma
entre eles e mostre o resultado da soma
    */
    inteiro num1, num2, soma
    escreva("Escreva o número 1: ")
    leia(num1)
    escreva("Escreva o número 2: ")
    leia(num2)
    soma = num1 + num2
    limpa()
    escreva("Soma de ",num1," com ",num2," é igual a ", soma)
  }
}
