programa {
  funcao inicio() {
    inteiro primeiro_numero
    inteiro segundo_numero
    inteiro soma
    escreva("Digite um número:\n")
    leia(primeiro_numero)
    escreva("Digite outro número:\n")
    leia(segundo_numero)
    soma = primeiro_numero + segundo_numero
    escreva("A soma entre os dois números é ",soma)
  }
}
