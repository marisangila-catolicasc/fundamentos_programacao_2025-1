programa {
  funcao inicio() {
    inteiro numero, antecessor, sucessor
    escreva("Digite um número e mostrarei seu antecessor e seu sucessor:")
    leia(numero)
    antecessor = numero - 1
    sucessor = numero + 1
    escreva("O número informado foi: ", numero, ", seu antecessor: ", antecessor, ", seu sucessor: ", sucessor )
     }
}
