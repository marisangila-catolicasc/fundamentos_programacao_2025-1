programa {
  funcao inicio() {
    real numero_um,numero_dois,soma
    escreva("Me informe um número: ")
    leia(numero_um)
    escreva("Me informe um segundo número: ")
    leia(numero_dois)
    soma = numero_um + numero_dois
    escreva("O resultado de ",numero_um," + ",numero_dois, " É:", soma)

  }
}
