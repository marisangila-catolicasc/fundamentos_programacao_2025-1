programa {
  funcao inicio() {
    inteiro valor_a
    inteiro valor_b
    inteiro inverso_a
    inteiro inverso_b

    escreva("Digite um valor\n")
    leia(valor_a)
    escreva("Digite outro valor\n")
    leia(valor_b)
   
    inverso_a = valor_b
    inverso_b = valor_a

    escreva("O 1° era valor: ", valor_a, " , o 2° valor era: ", valor_b, " Agora o 1° valor é ", inverso_a, ", e o 2° valor é: ", inverso_b)
    
  }
}
