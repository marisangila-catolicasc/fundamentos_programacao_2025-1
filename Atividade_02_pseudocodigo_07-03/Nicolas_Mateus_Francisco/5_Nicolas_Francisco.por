programa {
  funcao inicio() {
    real numero_1
    real numero_2
    real resultado_soma
    

    escreva("Soma Simples \n")
    escreva("Escreva o primeiro número: ")
    leia(numero_1)
    escreva("Escreva o segundo número: ")
    leia(numero_2)

    resultado_soma = numero_1 + numero_2
    
    escreva("\nO Resultado da soma é: ",resultado_soma)
  }
}
