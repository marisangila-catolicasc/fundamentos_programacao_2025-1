programa {
  funcao inicio() {
    inteiro idade

    real peso

    cadeia tipo

    escreva("Qual a idade do seu Pet?\n")
    leia(idade)

    escreva("Qual o peso do seu pet em Kg?\n")
    leia(peso)

    escreva("Seu Pet é um Gato (Insira G) ou um cachorro (Insira C)\n")
    leia(tipo)

    escreva("O Pet é do Tipo "+ tipo +" Tem a idade "+ idade +" e tem " +" peso " + peso + "Kg" )


  }
}
