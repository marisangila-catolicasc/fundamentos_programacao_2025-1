programa {
  funcao inicio() {
    inteiro idade
    real peso
    cadeia raca

    escreva("Insira a idade do seu Pet: ")
    leia(idade)
    escreva("Insira a altura do seu Pet em metros: ")
    leia(peso)
    escreva("Escreva C para cachorro ou G para gato:")
    leia(raca)
    escreva("C é cachorro e G gato, seu pet é: ", raca)
  }
}
