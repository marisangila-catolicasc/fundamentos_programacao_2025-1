programa {
  funcao inicio() {
    
    inteiro numero

    escreva("Informe um numero e descubra qual é seu numero antecessor e sussesor: \n")
    leia(numero)

    escreva("O numero selecionado é: (", numero,")! \n", "O seu antecessor é: (", numero - 1, "); \n", "O seu sucessor é: (", numero + 1,")", ".")

  }
}
