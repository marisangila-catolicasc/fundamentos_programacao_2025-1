programa {
  funcao inicio() {

    real valor_a
    real valor_b

    escreva("Informe o valor de A: ")
    leia(valor_a)

    escreva("Informe o valor de B: ")
    leia(valor_b)

    escreva("O seu valor de A passa a se: ", valor_b, "! \n")
    escreva("O seu valor de B passa a ser: ", valor_a, "! \n")

    escreva("Voce trocou o valor de A:","(", valor_a,")", " pelo valor de B:","(", valor_b,")", "!")

  }
}
