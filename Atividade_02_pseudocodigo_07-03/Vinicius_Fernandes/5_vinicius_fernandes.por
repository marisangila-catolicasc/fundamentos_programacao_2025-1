programa {
  funcao inicio() {
    real numero1
    real numero2
    real resultado_soma
    escreva("Digite um número: \n")
    leia(numero1)
    escreva("Digite outro número: \n")
    leia(numero2)

    resultado_soma = numero1 + numero2

    escreva("A soma entre os dois número é de: ",resultado_soma)
  }
}
