programa {
  funcao inicio() {
    inteiro idade
    real peso
    cadeia tipo
     escreva("Qual a idade do seu pet? \n")
     leia (idade)
     escreva("Qual o peso do seu pet? \n" )
     leia (peso)
     escreva("Qual tipo do seu pet? Se for cachorro, digite C ; Se for gato, digite G \n")
     leia(tipo)
     escreva("A idade do seu pet é:", idade)
     escreva(" \n O peso do seu pet é:", peso)
     escreva(" \n O tipo do seu pet é: ", tipo)
  }
}
